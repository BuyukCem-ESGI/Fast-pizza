,CAAC,CAAC,CAAC,CAAC;AACb","file":"/app/src/components/Register.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"row\">\n    <div class=\"col-md-4\"></div>\n    <div class=\"col-md-4\">\n      <div class=\"card\">\n        <div class=\"card-header\">\n          <h2 class=\"title\">REGISTER</h2>\n        </div>\n        <div class=\"card-body\">\n          <Form @submit=\"handleRegister\" :validation-schema=\"schema\">\n            <div v-if=\"!successful\">\n              <div class=\"form-group\">\n                <label for=\"firstname\">FirstName</label>\n                <Field name=\"firstname\" type=\"text\" class=\"form-control\"/>\n                <ErrorMessage name=\"firstname\" class=\"error-feedback\"/>\n              </div>\n              <div class=\"form-group\">\n                <label for=\"lastname\">lastname</label>\n                <Field name=\"lastname\" type=\"text\" class=\"form-control\"/>\n                <ErrorMessage name=\"lastname\" class=\"error-feedback\"/>\n              </div>\n              <div class=\"form-group\">\n                <label for=\"email\">Email</label>\n                <Field name=\"email\" type=\"email\" class=\"form-control\"/>\n                <ErrorMessage name=\"email\" class=\"error-feedback\"/>\n              </div>\n              <div class=\"form-group\">\n                <label for=\"phoneNumber\">Phone Number</label>\n                <Field name=\"phoneNumber\" type=\"text\" class=\"form-control\"/>\n                <ErrorMessage name=\"phoneNumber\" class=\"error-feedback\"/>\n              </div>\n              <div class=\"form-group\">\n                <label for=\"password\">Password</label>\n                <Field name=\"password\" type=\"password\" class=\"form-control\"/>\n                <ErrorMessage name=\"password\" class=\"error-feedback\"/>\n              </div>\n\n              <div class=\"form-group\">\n                <button class=\"btn btn-primary btn-block\" :disabled=\"loading\">\n                    <span\n                        v-show=\"loading\"\n                        class=\"spinner-border spinner-border-sm\"\n                    ></span>\n                  Sign Up\n                </button>\n              </div>\n            </div>\n          </Form>\n\n          <div\n              v-if=\"message\"\n              class=\"alert\"\n              :class=\"successful ? 'alert-success' : 'alert-danger'\"\n          >\n            {{ message }}\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"col-md-4\"></div>\n  </div>\n</template>\n\n<script>\nimport {Form, Field, ErrorMessage} from \"vee-validate\";\nimport * as yup from \"yup\";\n\nexport default {\n  name: \"Register\",\n  components: {\n    Form,\n    Field,\n    ErrorMessage,\n  },\n  data() {\n    return {\n      loading: false,\n      successful: false,\n      message: null\n    };\n  },\n  setup() {\n    console.log(self);\n    return {\n      schema: yup.object({\n        firstname: yup.string().required(),\n        lastname: yup.string().required(),\n        email: yup.string().email().required(),\n        phoneNumber: yup.string().required(),\n        password: yup.string().required().min(8).max(255)\n      }),\n      handleRegister(user) {\n        self.loading = true;\n        self.message = \"\";\n        self.successful = false;\n        if (user) {\n          self.$store.dispatch(\"auth/register\", user)\n              .then(() => {\n                    self.successful = true;\n                    self.message = \"You have successfully registered!\";\n                    self.loading = false;\n                  },\n              ).catch(() => {\n                self.message = \"Something went wrong!\"\n                self.successful = false;\n                self.loading = false;\n              }\n          );\n        } else {\n          self.successful = false;\n          self.message = \"Please fill out all fields correctly!\";\n          self.loading = false;\n        }\n      }\n    };\n  },\n  created() {\n    self = this;\n  },\n  computed: {\n    loggedIn() {\n      return this.$store.state.auth.status.loggedIn;\n    },\n  },\n  mounted() {\n    if (this.loggedIn) {\n      this.$router.push(\"/profile\");\n    }\n  },\n};\n</script>\n\n<style scoped>\n.btn-primary {\n  background-color: #1AC073 !important;\n  border-color: #1AC073 !important;\n  font-weight: bold;\n  font-size: 20px\n}\n\n.row {\n  padding-top: 10vh\n}\n\n.card-header {\n  background-color: #1AC073\n}\n\n.title {\n  font-weight: bold;\n  text-align: center;\n  color: #fff;\n}\n</style>\n"]}]}