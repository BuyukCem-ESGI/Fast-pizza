 </div>\n      <div class=\"col-md-1\"></div>\n    </div>\n    <div class=\"row component-row\">\n      <div class=\"col-xs-2 col-md-1\"></div>\n        <!-- Inactive components will be cached! -->\n        <keep-alive>\n          <component :is=\"currentTabComponent\">     </component>\n        </keep-alive>\n      <div class=\"col-xs-2 col-md-1\"></div>\n    </div>\n</template>\n\n<script>\nimport Menu from \"./Menu.vue\";\nimport Pizzas from \"./Pizzas.vue\";\nimport Extra from \"./Extra.vue\";\n\nexport default {\n  name: \"Home\",\n  data() {\n    return {\n      content: \"\",\n      currentTab: 'Menu',\n      tabs: ['Menu','Pizzas', 'Extra']\n    }\n  },\n  computed: {\n    currentTabComponent() {\n      return this.currentTab;\n    }\n  },\n  components: {\n    Menu,\n    Pizzas,\n    Extra\n  }\n};\n</script>\n\n<style>\n  .header {\n    height: 65vh;\n    color: red;\n    padding: 5rem 0 0 0;\n    background-color: #51d27b\n  }\n  .big-text {\n    font-family: Gilroy-Medium;\n    font-size: 5vw;\n    line-height: 6vw;\n    color: #FFFFFF;\n  }\n  .header-left {\n    padding-top: 30px\n  }\n  .header-right {\n  }\n  .ctl {\n    background-color: blue\n  }\n  .small-text {\n    font-family: SF Pro Text;\n    font-style: normal;\n    font-weight: normal;\n    font-size: 25px;\n    line-height: 25px;\n    letter-spacing: -0.015em;\n    color: #FFFFFF;\n    flex: none;\n    order: 1;\n    flex-grow: 0;\n    margin-top: 2rem;\n  }\n  .btn-warning {\n    color: #ffffff !important\n  }\n  .search-form {\n    margin-top: 20px;\n  }\n  .tabs-row {\n    margin-top: 6vw;\n  }\n  .component-row {\n    margin-top: 2vw;\n    padding-bottom: 8vw;\n  }\n  .foods-text {\n    font-family: Gilroy-Medium;\n    font-size: 34px;\n    line-height: 24px;\n    letter-spacing: -0.012em;\n    color: #000000;\n  }\n  .tab-button {\n    padding: 10px 25px;\n    border-top-left-radius: 3px;\n    border-top-right-radius: 3px;\n    cursor: pointer;\n    margin-bottom: -1px;\n    margin-right: -1px;\n    border: 1px solid #F3BA00;\n    color: #929292;\n    font-size: 20px;\n    font-weight: bold;\n  }\n  .tab-button:hover {\n    background: #F3BA00;\n    color: #000000;\n  }\n  .tab-button.active {\n    background: #F3BA00;\n    color: #000000;\n  }\n\n  @media only screen and (max-width: 768px) {\n  /* For mobile phones: */\n  .big-text {\n    font-size: 8vw;\n    line-height: 9vw;\n  }\n  .small-text {\n    font-size: 16px;\n    line-height: 10px;\n  }\n  .hide {\n    display: none\n  }\n  .header {\n    height: 30vh;\n    padding: 2rem 0 0 0\n  }\n  .input-search {\n    width: 70% !important;\n    margin-right: 5px;\n  }\n  .search-form {\n    margin-top: 15px\n  }\n    .foods-text {\n    font-family: Gilroy-Medium;\n    font-size: 28px;\n    line-height: 18px;\n  }\n}\n</style>\n"]}]}